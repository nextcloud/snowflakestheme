    <id>snowflakestheme</id>
    <name>Snowflakes Theme</name>
    <summary>A simple app to show some snowflakes in the nextcloud web interface</summary>
    <description><![CDATA[This app just adds an overlay over all nextcloud pages to show a snowing effect.]]></description>
    <version>1.1.4</version>
    <licence>agpl</licence>
    <author mail="github@christianwolf.email" >Christian Wolf</author>
    <namespace>SnowflakesTheme</namespace>
    <category>customization</category>
    <website>https://github.com/christianlupus-nextcloud/snowflakestheme</website>
    <bugs>https://github.com/christianlupus-nextcloud/snowflakestheme/issues</bugs>
    <screenshot>https://raw.githubusercontent.com/christianlupus-nextcloud/snowflakestheme/main/img/Screencast.gif</screenshot>
    <dependencies>
        <nextcloud min-version="31" max-version="33"/>
    </dependencies>
    <settings>
        <admin>OCA\SnowflakesTheme\Settings\AdminSettings</admin>
        <personal>OCA\SnowflakesTheme\Settings\UserSettings</personal>
    </settings>
</info>
